{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": "ses:SendEmail",
            "Resource": "arn:aws:ses:<REGION>:<ACCOUNT>:identity/<EMAIL>"
        },
        {
            "Effect": "Allow",
            "Action": "ses:SendEmail",
            "Resource": "arn:aws:ses:<REGION>:<ACCOUNT>:configuration-set/<CONFIG_SET_NAME>"
        }
    ]
}